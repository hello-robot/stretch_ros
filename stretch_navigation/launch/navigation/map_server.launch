<launch>
  <!-- Doc: http://wiki.ros.org/map_server?distro=kinetic -->

  <arg name="map_name" default="map" />

  <!-- launch map server -->
	<node name="map_server" pkg="map_server" type="map_server"
        args="$(find stretch_navigation)/resources/static_maps/2d/$(arg map_name).yaml"/>
</launch>